<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.*?>


<StackPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.fei.gui.PrincipalAdministradorController">
    <stylesheets>
        <URL value="@principaladministrador.css"/>
    </stylesheets>
    
    <ImageView fitHeight="400"
               fitWidth="500"
               StackPane.alignment="CENTER_LEFT">
        <image>
            <Image url="mx/fei/images/coming-soon.jpg" />
        </image>
    </ImageView>
    
    <JFXButton fx:id="agregarAuto"
               text="Agregar Auto"
               prefHeight="30"
               prefWidth="100"
               StackPane.alignment="CENTER_RIGHT"
               onAction="#abrirAgregarAuto">
        
        <StackPane.margin>
            <Insets right="10"/>
        </StackPane.margin>
        
    </JFXButton>
    
</StackPane>
